{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Untyped",
  "colors": {
    "primary": "#0081f3",
    "light": "#0081f3",
    "dark": "#0081f3"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "rocket",
            "pages": [
              "index",
              "quickstart",
              {
                "group": "Connecting Your Email",
                "pages": ["connecting-email/general", "connecting-email/gmail", "connecting-email/outlook"]
              }
            ]
          },
          {
            "group": "Functionality",
            "icon": "wand-magic-sparkles",
            "pages": [
              "features/drafting",
              "features/labels",
              "features/knowledge-base",
              {
                "group": "Prompt Editor",
                "pages": ["features/prompt-editor", "features/prompt-editor/components", "features/prompt-editor/best-practices", "features/prompt-editor/templates"]
              },
              {
                "group": "Rules",
                "pages": ["features/rules", "features/exclude-prompt"]
              }
            ]
          },
          {
            "group": "Settings",
            "icon": "gear",
            "pages": ["settings/signature", "settings/profile", "settings/mailboxes", "settings/billing"]
          },
          {
            "group": "Data & Privacy",
            "icon": "shield",
            "pages": [
              {
                "group": "Your Data",
                "pages": ["data-privacy/your-data", "data-privacy/diagnostics"]
              },
              "data-privacy/security"
            ]
          },
          {
            "group": "Support",
            "icon": "life-ring",
            "pages": [
              "support",
              "faqs",
              {
                "group": "Troubleshooting",
                "pages": ["troubleshooting/outlook-settings", "troubleshooting/microsoft-365-admin-setup"]
              }
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Updates",
            "icon": "clock-rotate-left",
            "pages": ["changelog"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Go to App",
          "href": "https://app.getuntyped.ai",
          "icon": "arrow-up-right-from-square"
        },
        {
          "anchor": "Go to Website",
          "href": "https://www.getuntyped.ai",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://docs.getuntyped.ai/support"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Go to App",
      "href": "https://app.getuntyped.ai"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/getuntypedai",
      "linkedin": "https://www.linkedin.com/company/107197819"
    }
  }
}
