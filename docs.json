{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Untyped",
  "colors": {
    "primary": "#0081f3",
    "light": "#0081f3",
    "dark": "#0081f3"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "quickstart", "connecting-email"]
          },
          {
            "group": "Features",
            "pages": ["features/drafting", "features/agent", "features/labels", "features/prompt-editor", "features/rules", "features/knowledge"]
          },
          {
            "group": "Settings",
            "pages": ["settings/profile", "settings/mailboxes", "settings/billing"]
          },
          {
            "group": "Data & Privacy",
            "pages": ["data-privacy/your-data", "data-privacy/security", "data-privacy/export"]
          },
          {
            "group": "Updates",
            "pages": ["changelog"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Go to App",
          "href": "https://beta.getuntyped.ai",
          "icon": "arrow-up-right-from-square"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@getuntyped.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Go to App",
      "href": "https://beta.getuntyped.ai"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/getuntypedai"
    }
  }
}
