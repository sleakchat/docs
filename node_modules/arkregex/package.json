{
  "name": "arkregex",
  "description": "A drop-in replacement for new RegExp() with types",
  "version": "0.0.2",
  "license": "MIT",
  "author": {
    "name": "David Blass",
    "email": "david@arktype.io",
    "url": "https://arktype.io"
  },
  "type": "module",
  "main": "./out/index.js",
  "types": "./out/index.d.ts",
  "exports": {
    ".": {
      "ark-ts": "./index.ts",
      "default": "./out/index.js"
    },
    "./internal/*.ts": {
      "ark-ts": "./*.ts",
      "default": "./out/*.js"
    },
    "./internal/*.js": {
      "ark-ts": "./*.ts",
      "default": "./out/*.js"
    }
  },
  "files": [
    "out"
  ],
  "dependencies": {
    "@ark/util": "0.53.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "ts ../repo/build.ts",
    "test": "ts ../repo/testPackage.ts",
    "bench": "ts ./__tests__/regex.bench.ts"
  }
}