{
  "name": "@mintlify/cli",
  "version": "4.0.790",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mint": "bin/index.js",
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && NODE_NO_WARNINGS=1 node bin/index.js",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@mintlify/common": "1.0.592",
    "@mintlify/link-rot": "3.0.733",
    "@mintlify/models": "0.0.238",
    "@mintlify/prebuild": "1.0.720",
    "@mintlify/previewing": "4.0.769",
    "@mintlify/validation": "0.1.513",
    "adm-zip": "^0.5.10",
    "chalk": "^5.2.0",
    "color": "^4.2.3",
    "detect-port": "^1.5.1",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "ink": "^6.0.1",
    "inquirer": "^12.3.0",
    "js-yaml": "^4.1.0",
    "mdast": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "react": "^19.1.0",
    "semver": "^7.7.2",
    "unist-util-visit": "^5.0.0",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config-typescript": "1.0.13",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/adm-zip": "^0.5.7",
    "@types/detect-port": "^1.3.2",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "openapi-types": "^12.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.4",
    "vitest-mock-process": "^1.0.4"
  },
  "gitHead": "1b33af402d791fb3650721358cb56c0f511a3806"
}
